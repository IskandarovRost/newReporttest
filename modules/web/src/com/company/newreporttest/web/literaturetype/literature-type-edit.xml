<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.company.newreporttest.web.literaturetype.LiteratureTypeEdit"
        datasource="literatureTypeDs"
        focusComponent="fieldGroup"
        messagesPack="com.company.newreporttest.web.literaturetype">
    <dsContext>
        <datasource id="literatureTypeDs"
                    class="com.company.newreporttest.entity.LiteratureType"
                    view="_local"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="literatureTypeDs">
            <column width="250px">
                <field id="name"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
